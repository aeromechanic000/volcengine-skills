{
  "name": "volcengine-skills",
  "version": "1.0.0",
  "description": "Unified Volcengine Image and Video generation tools",
  "author": {
    "name": "Aeromechanic",
    "url": "https://aeromechanic.fun/"
  },
  "mcpServers": {
    "volc-img": {
      "command": "npx",
      "args": ["-y", "@aeromechanic/volcengine-image-mcp"],
      "env": {
        "DOUBAO_API_KEY": "YOUR_API_KEY_HERE"
      }
    },
    "volc-vid": {
      "command": "npx",
      "args": ["-y", "@aeromechanic/volcengine-video-mcp"],
      "env": {
        "DOUBAO_API_KEY": "YOUR_API_KEY_HERE"
      }
    }
  },
  "repository": "https://aeromechanic000/volcengine-skills",
  "license": "MIT",
  "keywords": [
    "volcengine",
    "image",
    "video",
    "seedream",
    "seedance"
  ]
}
